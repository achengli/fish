function __alias_fish
	alias reload="source ~/.config/fish/config.fish"
	alias emacs="/usr/bin/emacs -nw"
	alias elisp="/usr/bin/emacs --script"
	alias lisp=ros
	alias cl=clisp
	alias js=node
	alias r=R
	alias rscript=Rscript
	alias vi=/usr/bin/vim
	alias octave="octave-cli -q"
	alias sbcl="rlwrap /usr/bin/sbcl"
	alias ngspice="rlwrap ngspice"
	alias enes='trans en:es'
	alias esen='trans es:en'
end
